Cell["\<\
EnacbaNosilke[Daljica[AA_, BB_]], ki rne ena\[CHacek]bo premice nosilke \
daljice. Primer, za daljico d kot zgoraj je ena\[CHacek]ba nosilke y == -x/2 \
+ 1/2\
\>", "Item",
 CellChangeTimes->{{3.751020114967822*^9, 
  3.7510201555915623`*^9}},ExpressionUUID->"a863a116-188b-4cbe-9dd6-\
97390a6061c7"],

Cell[BoxData[{
 RowBox[{"ClearAll", "[", 
  RowBox[{"x", ",", " ", "y"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"EnacbaNosilke", "[", 
   RowBox[{"Daljica", "[", 
    RowBox[{"AA_", ",", "BB_"}], "]"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "x1", ",", " ", "y1", ",", " ", "x2", ",", " ", "y2", ",", " ", "k", ",",
       " ", "n"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x1", ",", " ", "y1"}], "}"}], "=", "AA"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x2", ",", " ", "y2"}], "}"}], "=", "BB"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"k", " ", "=", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"y2", " ", "-", " ", "y1"}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"x2", " ", "-", " ", "x1"}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"n", " ", "=", 
      RowBox[{"n", " ", "/.", 
       RowBox[{"First", "[", 
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{"y1", " ", "==", " ", 
           RowBox[{
            RowBox[{"k", "*", "x1"}], " ", "+", " ", "n"}]}], ",", " ", "n"}],
          "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"y", " ", "==", "  ", 
      RowBox[{
       RowBox[{"k", "*", "x"}], " ", "+", " ", "n"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
